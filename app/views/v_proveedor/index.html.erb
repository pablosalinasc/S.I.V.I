<h1>Listing v_proveedor</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Id proveedor</th>
      <th>Nombre proveedor</th>
      <th>Pais proveedor</th>
      <th>Ciudad proveedor</th>
      <th>Direccion proveedor</th>
      <th>Correo proveedor</th>
      <th>Cantidad cotizaciones</th>
      <th>Cantidad ordenes de compra</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_proveedor.each do |v_proveedor| %>
      <tr>
        <td><%= v_proveedor.ID_PROVEEDOR %></td>
        <td><%= v_proveedor.NOMBRE_PROVEEDOR %></td>
        <td><%= v_proveedor.PAIS_PROVEEDOR %></td>
        <td><%= v_proveedor.CIUDAD_PROVEEDOR %></td>
        <td><%= v_proveedor.DIRECCION_PROVEEDOR %></td>
        <td><%= v_proveedor.CORREO_PROVEEDOR %></td>
        <td><%= v_proveedor.CANTIDAD_COTIZACIONES %></td>
        <td><%= v_proveedor.CANTIDAD_ORDENES_DE_COMPRA %></td>
        <td><%= link_to 'Show', v_proveedor %></td>
        <td><%= link_to 'Edit', edit_v_proveedor_path(v_proveedor) %></td>
        <td><%= link_to 'Destroy', v_proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

