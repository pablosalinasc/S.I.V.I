<h1>Listing tipo_insumo</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Nombre insumo</th>
      <th>Compatibilidad insumo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_insumo.each do |tipo_insumo| %>
      <tr>
        <td><%= tipo_insumo.NOMBRE_INSUMO %></td>
        <td><%= tipo_insumo.COMPATIBILIDAD_INSUMO %></td>
        <td><%= link_to 'Show', tipo_insumo %></td>
        <td><%= link_to 'Edit', edit_tipo_insumo_path(tipo_insumo) %></td>
        <td><%= link_to 'Destroy', tipo_insumo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo insumo', new_tipo_insumo_path %>
