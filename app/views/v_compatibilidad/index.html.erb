<h1>Listing v_compatibilidad</h1>

<table>
  <thead>
    <tr>
      <th>Id insumo</th>
      <th>Id marca</th>
      <th>Nombre marca</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_compatibilidad.each do |v_compatibilidad| %>
      <tr>
        <td><%= v_compatibilidad.ID_INSUMO %></td>
        <td><%= v_compatibilidad.ID_MARCA %></td>
        <td><%= v_compatibilidad.NOMBRE_MARCA %></td>
        <td><%= link_to 'Show', v_compatibilidad %></td>
        <td><%= link_to 'Edit', edit_v_compatibilidad_path(v_compatibilidad) %></td>
        <td><%= link_to 'Destroy', v_compatibilidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V compatibilidad', new_v_compatibilidad_path %>
