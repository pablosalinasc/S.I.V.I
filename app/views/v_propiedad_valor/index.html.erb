<h1>Listing v_propiedad_valor</h1>

<table>
  <thead>
    <tr>
      <th>Id insumo</th>
      <th>Id propiedad</th>
      <th>Nombre propiedad</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_propiedad_valor.each do |v_propiedad_valor| %>
      <tr>
        <td><%= v_propiedad_valor.ID_INSUMO %></td>
        <td><%= v_propiedad_valor.ID_PROPIEDAD %></td>
        <td><%= v_propiedad_valor.NOMBRE_PROPIEDAD %></td>
        <td><%= v_propiedad_valor.VALOR %></td>
        <td><%= link_to 'Show', v_propiedad_valor %></td>
        <td><%= link_to 'Edit', edit_v_propiedad_valor_path(v_propiedad_valor) %></td>
        <td><%= link_to 'Destroy', v_propiedad_valor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

