<h1>Listing codigo_proveedor</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Id proveedor</th>
      <th>Id insumo</th>
      <th>Codigo insumo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @codigo_proveedor.each do |codigo_proveedor| %>
      <tr>
        <td><%= codigo_proveedor.ID_PROVEEDOR %></td>
        <td><%= codigo_proveedor.ID_INSUMO %></td>
        <td><%= codigo_proveedor.CODIGO_INSUMO %></td>
        <td><%= link_to 'Show', codigo_proveedor %></td>
        <td><%= link_to 'Edit', edit_codigo_proveedor_path(codigo_proveedor) %></td>
        <td><%= link_to 'Destroy', codigo_proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

