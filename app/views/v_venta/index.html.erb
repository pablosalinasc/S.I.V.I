<h1>Listing v_venta</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Id venta</th>
      <th>Nombre vendedor</th>
      <th>Nombre cliente</th>
      <th>Direccion local</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @v_venta.each do |v_venta| %>
      <tr>
        <td><%= v_venta.ID_VENTA %></td>
        <td><%= v_venta.NOMBRE_VENDEDOR %></td>
        <td><%= v_venta.NOMBRE_CLIENTE %></td>
        <td><%= v_venta.DIRECCION_LOCAL %></td>
        <td><%= link_to 'Show', v_venta %></td>
        <td><%= link_to 'Edit', edit_v_venta_path(v_venta) %></td>
        <td><%= link_to 'Destroy', v_venta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New V venta', new_v_venta_path %>
