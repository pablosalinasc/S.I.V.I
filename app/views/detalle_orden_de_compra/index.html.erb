        <td><%= link_to 'Edit', edit_detalle_orden_de_compra_path(detalle_orden_de_compra) %></td>
        <td><%= link_to 'Destroy', detalle_orden_de_compra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
