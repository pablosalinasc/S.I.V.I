<h1>Listing proveedor</h1>

<table>
  <thead>
    <tr>
      <th>Nombre proveedor</th>
      <th>Pais proveedor</th>
      <th>Ciudad proveedor</th>
      <th>Direccion proveedor</th>
      <th>Correo proveedor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proveedor.each do |proveedor| %>
      <tr>
        <td><%= proveedor.NOMBRE_PROVEEDOR %></td>
        <td><%= proveedor.PAIS_PROVEEDOR %></td>
        <td><%= proveedor.CIUDAD_PROVEEDOR %></td>
        <td><%= proveedor.DIRECCION_PROVEEDOR %></td>
        <td><%= proveedor.CORREO_PROVEEDOR %></td>
        <td><%= link_to 'Show', proveedor %></td>
        <td><%= link_to 'Edit', edit_proveedor_path(proveedor) %></td>
        <td><%= link_to 'Destroy', proveedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proveedor', new_proveedor_path %>
